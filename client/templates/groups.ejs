<div class="container content mx-auto">
	<h1 class="text-2xl">Your Groups</h1>
	<ul class="mt-4">
		<%groups.forEach((group)=>{%>
		<li class="mb-2">
			<a class="bg-black p-1 text-white hover:underline" href="/groups/<%=group.custom_path || group.id%>"><%=group.name%></a>
			<p class="mt-1"><%=group.description%></p>
		</li>
		<%})%>
	</ul>
</div>
